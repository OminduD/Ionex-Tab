{
    "manifest_version": 2,
    "name": "Ionex Tab - Enhanced New Tab",
    "version": "1.0.0",
    "description": "A feature-rich, customizable new tab page with widgets, AI assistance, and beautiful themes.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "ionex-tab@omindu.dev",
            "strict_min_version": "109.0"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}