{
  "manifest_version": 3,
  "name": "My Enhanced Home Tab",
  "version": "1.0",
  "description": "A feature-rich, customizable new tab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.openweathermap.org/",
    "https://generativelanguage.googleapis.com/",
    "https://*.google.com/"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
